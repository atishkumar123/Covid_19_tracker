<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.atish.covid19_tracker">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/coronavirus"
        android:label="@string/app_name"
        android:roundIcon="@drawable/coronavirus"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splash_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".State_Details_Activity" />
        <activity android:name=".District_Data_Activity" />
        <activity android:name=".StateList_Activity" />
        <activity android:name=".countryDetails_Activity" />
        <activity android:name=".countries_list_Activity" />
        <activity android:name=".MainActivity"/>



        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>